{
  "name": "explorer",
  "version": "1.0.0",
  "description": "A simple package to simplify node fs.",
  "main": "dist/index.js",
  "scripts": {
    "push": "npm run compile & git add . & git commit -m \"Save\" & git push",
    "compile": "npm run clean & tsc -p .",
    "clean": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\""
  },
