  "bugs": {
    "url": "https://github.com/TuncayBasak25/explorer/issues"
  },
  "homepage": "https://github.com/TuncayBasak25/explorer#readme",
  "devDependencies": {
    "@types/node": "^17.0.22",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "query": "github:TuncayBasak25/query",
    "ts-mixer": "^6.0.1"
  }
}
